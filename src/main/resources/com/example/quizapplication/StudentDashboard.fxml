<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.quizapplication.StudentDashboardController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" layoutY="59.0" pickOnBounds="true" preserveRatio="true" scaleY="1.5">
         <image>
            <Image url="@../../../Login%20Background.jpg" />
         </image>
      </ImageView>
      <Button layoutX="69.0" layoutY="175.0" mnemonicParsing="false" onAction="#handlestartquiz" prefHeight="25.0" prefWidth="168.0" style="-fx-background-color: white;" text="Start Quiz" />
      <Button layoutX="69.0" layoutY="222.0" mnemonicParsing="false" onAction="#handleviewprogress" prefHeight="25.0" prefWidth="168.0" text="View Progress" />
      <Label layoutX="31.0" layoutY="45.0" text="Student Dashboard" textFill="WHITE">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="BackButton" layoutX="510.0" layoutY="32.0" mnemonicParsing="false" onAction="#handlebackbutton" text="SignOut" />
   </children>
</AnchorPane>
